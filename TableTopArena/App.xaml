             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="XamlResources/Main.xaml" />
            </ResourceDictionary.MergedDictionaries>

            <Style TargetType="{x:Type Rectangle}">
            </Style>
        </ResourceDictionary>
    </Application.Resources>    
